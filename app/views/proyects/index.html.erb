<header class="masthead clear">
  <div class="centered">
    <div class="site-branding">
      <h1 class="site-title">Projects</h1>
    </div>
  </div>
</header>


<div class="main-area">
  <div class="centered">
    <section class="cards">
      <% if Proyect.count != 0%>
        <% @proyects.each do |project| %>
          <%= render 'preview', project: project%>
        <% end %>
      <% else %>
        <p>No proyects yet</p>
      <% end %>
    </section>
  </div>
</div>
